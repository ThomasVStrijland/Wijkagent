        <Button x:Name="DelictList" Content="Delicten lijst" HorizontalAlignment="Left" Margin="10,83,0,0" VerticalAlignment="Top" Width="214" Height="68" Click="DelictList_Click"/>
        <Button x:Name="AddUser" Content="Nieuwe gebruiker" HorizontalAlignment="Left" Margin="10,341,0,0" VerticalAlignment="Top" Width="214" Height="68" Click="AddUser_Click"/>
        <Button x:Name="LogOut" Content="Log-uit" HorizontalAlignment="Left" Margin="672,380,0,0" VerticalAlignment="Top" Width="110" Height="29" Click="LogOut_Click"/>
    </Grid>
</Window>
